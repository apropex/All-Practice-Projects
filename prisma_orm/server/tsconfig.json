{
  "compilerOptions": {
    "target": "es2016",
    "module": "es2022",
    "rootDir": ".",
    "outDir": "dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "typeRoots": ["./node_modules/@types", "./types"],
    "types": ["node"]
  }
  // "include": ["src/**/*", "types/**/*"]
}
